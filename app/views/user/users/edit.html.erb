<%= form_with model:@user,url:user_user_path,method: :patch, local:true do |f| %>
  <%= f.label :image,'ニックネーム' %>
  <%= f.text_field:nickname %>
  <%= f.label :image,'メールアドレス' %>
  <%= f.text_field:email %>


  <%= f.submit"編集内容を保存",class:'btn btn-success' %>
  <%= link_to"退会する",users_comfirmation_path,class:"btn btn-sm btn-danger" %>
<% end %>